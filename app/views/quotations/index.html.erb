<style>
  .select2-selection__choice{
    display: none!important;
  }

  .select2-selection--single{
    background: #f6f6f6!important; 
  }

  @media(max-width: 992px){
    .select2-selection--single{
      background: #EEEEEE!important; 
    }
  }

  .select2-container--default .select2-selection--single{
    border: 2px solid #B3B3B3!important;
  }

  .select2-selection__rendered{
    padding-left: 10px!important;
  }

  .select2-miele .select2-container {
    width: calc(100% - 60px) !important;
  }
</style>
<div class="row">
  <div class="col-lg-12">
    <% if current_user.manager? or current_user.administrator? %>
      <ol class="breadcrumb">
        <li><a href="<%= root_path %>">Inicio</a></li>
        <li><%= get_title(current_user) %></li>
      </ol>
    <% end %>
    <h1 class="title-section bold">
      <%= get_title(current_user)  %>
    </h1>
  </div>
</div>
<%= render 'quotations/quotations' %>